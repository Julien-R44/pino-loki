{
  "name": "pino-loki",
  "version": "2.1.1",
  "type": "module",
  "description": "A transport for pino that sends messages to Loki",
  "keywords": [
    "pino",
    "pino-transport",
    "loki",
    "logging"
  ],
  "homepage": "https://github.com/Julien-R44/pino-loki#readme",
  "bugs": {
    "url": "https://github.com/Julien-R44/pino-loki/issues"
  },
  "packageManager": "pnpm@8.5.1",
  "license": "MIT",
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-R44/pino-loki.git"
  },
  "bin": {
    "pino-loki": "dist/cli.cjs"
  },
  "funding": "https://github.com/sponsors/Julien-R44",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "stub": "unbuild --stub",
    "quick:test": "node --loader=ts-node/esm bin/test.ts",
    "test": "c8 node --loader=ts-node/esm bin/test.ts",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@japa/assert": "^1.4.1",
    "@japa/runner": "^2.5.1",
    "@japa/spec-reporter": "^1.3.3",
    "@julr/prettier-config": "1.0.0-beta.2",
    "@types/node": "^20.2.1",
    "@types/pump": "^1.1.1",
    "bumpp": "^9.1.0",
    "c8": "^7.13.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "nock": "^13.3.1",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "pnpm": "^8.5.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build",
    "dist"
  ],
  "prettier": "@julr/prettier-config",
  "dependencies": {
    "commander": "^10.0.1",
    "got": "^11.8.6",
    "pino-abstract-transport": "^1.0.0",
    "pump": "^3.0.0"
  }
}
